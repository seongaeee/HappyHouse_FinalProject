<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.happyhouse.dao.HouseDao">

	<select id="selectAll" resultType="HouseInfo">
	    select * from houseinfo
	</select>

	<select id="selectDong" parameterType="java.lang.String" resultType="HouseInfo">
	    select * from houseinfo where dong = #{dong}
	</select>
	
	<select id="selectApt" parameterType="java.lang.String" resultType="HouseInfo">
	    select * from houseinfo where AptName = #{apt} order by buildYear desc
	</select>
	
</mapper>