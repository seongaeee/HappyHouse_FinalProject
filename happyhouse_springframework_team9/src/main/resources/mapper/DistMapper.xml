<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.happyhouse.dao.DistDao">
	<select id="stationSelectAll" resultType="Position">
		select * from station where lat is not null and lng is not null
	</select>
	
	<select id="parkSelectAll" resultType="Position">
		select * from park where lat is not null and lng is not null
	</select>
</mapper>